# AI Provider: openai or azure
AI_PROVIDER=openai

# OpenAI Settings
OPENAI_API_KEY=your_openai_key
OPENAI_API_URL=https://api.openai.com/v1/chat/completions

# Azure OpenAI Settings (optional)
AZURE_API_KEY=your_azure_key
AZURE_DEPLOYMENT_ID=your_model
AZURE_API_URL=https://your-resource.openai.azure.com/openai/deployments/your_model/chat/completions?api-version=2024-02-15-preview

# Moodle Settings
MOODLE_URL=https://yourmoodle.com
MOODLE_WS_TOKEN=your_moodle_token
MOODLE_WS_FORMAT=json
